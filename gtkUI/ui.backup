<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object class="GtkApplicationWindow" id="window">
        <property name="title">DiskMark for Linux</property>
        <property name="default-width">500</property>
        <property name="default-height">400</property>
        <child>
            <object class="GtkBox">
                <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
                <child>
                    <object class="GtkOverlay">
                        <child>
                            <object class="GtkProgressBar" id="p1">
                            </object>
                        </child>
                        <child type="overlay">
                            <object class="GtkLabel" id="my_label">
                                <property name="label">LOAD SPEED</property>
                                <property name="css-classes">my_label</property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkButton">
                        <property name="label">Btn</property>
                    </object>
                </child>
                <child>
                    <object class="GtkProgressBar" id="p2">
                        <property name="text">Test</property>
                        <property name="show-text">TRUE</property>
                    </object>
                </child>
            </object>
        </child>
    </object>
</interface>